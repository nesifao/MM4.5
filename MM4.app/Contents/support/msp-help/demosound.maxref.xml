<c74object name="demosound" module="msp" category="MSP" kind="patcher">
	<digest>Sound source module for bpatcher</digest>
	<description>
		<o>demosound</o> is an abstraction/bpatcher that's easy to drop into patcher and starting working.
	</description>
	
	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Cycling '74</metadata>
		<metadata name="tag">MSP</metadata>
	</metadatalist>
	

	<!--ARGUMENTS-->
	<objarglist>

		<objarg name="number-of-FFT-samples" optional="0" type="int">
			<digest>Number of FFT points</digest>
			<description>
				The first argument specifies the number of points (samples) in the FFT.  It must be a power of two.  If you are unsure, use 1024.
			</description>
		</objarg>

	</objarglist>
	

	<!--MESSAGES-->
	<methodlist>
		
<!--
		<method name="signal">
			<arglist />
			<digest>Audio signal to be transformed</digest>
			<description>
				Audio signal to be transformed from the time domain into the frequency domain.
			</description>
		</method>
	-->	
	</methodlist>
	
	
	<!--ATTRIBUTES-->


	<!--SEEALSO-->
	<seealsolist>
		<seealso name="demovideo" />
	</seealsolist>


</c74object>
